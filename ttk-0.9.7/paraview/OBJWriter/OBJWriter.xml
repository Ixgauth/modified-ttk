<ServerManagerConfiguration>

  <ProxyGroup name="writers">
    <!-- ================================================================== -->
    <WriterProxy name="OBJWriter" class="ttkOBJWriter" label="OBJWriter">
        <Documentation
          long_help="Export a VTK Unstructured Grid into a Wavefront OBJ file." 
          short_help="Write an .obj file.">
        </Documentation>
        <InputProperty name="Input" command="SetInputConnection">
          <ProxyGroupDomain name="groups">
            <Group name="sources"/>
            <Group name="filters"/>
          </ProxyGroupDomain>
          <DataTypeDomain name="input_type" composite_data_supported="0">
            <DataType value="vtkDataSet"/>
          </DataTypeDomain>
        </InputProperty>
        <StringVectorProperty
          name="FileName"
          command="SetFileName"
          number_of_elements="1">
          <FileListDomain name="files"/>
          <Documentation>
              This property specifies the file name for the OBJ writer.
          </Documentation>
        </StringVectorProperty>
        <Hints>
          <Property name="Input" show="0"/>
          <Property name="FileName" show="0"/>
          <WriterFactory extensions="obj"
            file_description="Wavefront OBJ file" />
        </Hints>
    </WriterProxy>
    <!-- End Writer -->
  </ProxyGroup>

</ServerManagerConfiguration>
